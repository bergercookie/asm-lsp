<instructionsection id="MOV_sel_z_p_zz" title="MOV (vector, predicated)" type="alias">
  <docvars>
    <docvar key="alias_mnemonic" value="MOV" />
    <docvar key="instr-class" value="sve" />
    <docvar key="isa" value="A64" />
