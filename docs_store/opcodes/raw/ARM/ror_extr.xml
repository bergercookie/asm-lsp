      <encoding name="ROR_EXTR_32_extract" oneofinclass="2" oneof="2" label="32-bit" bitdiffs="sf == 0 &amp;&amp; N == 0 &amp;&amp; imms == 0xxxxx">
        <docvars>
          <docvar key="datatype" value="32"/>
          <docvar key="isa" value="A64"/>
          <docvar key="mnemonic" value="EXTR"/>
          <docvar key="alias_mnemonic" value="ROR"/>
          <docvar key="instr-class" value="general"/>
        </docvars>
        <box hibit="31" width="1" name="sf">
          <c>0</c>
        </box>
        <box hibit="22" width="1" name="N">
          <c>0</c>
        </box>
        <box hibit="15" width="6" name="imms">
          <c>0</c>
          <c/>
          <c/>
          <c/>
          <c/>
          <c/>
        </box>
        <asmtemplate><text>ROR  </text><a hover="Is the 32-bit name of the general-purpose destination register, encoded in the &quot;Rd&quot; field." link="WdOrWZR">&lt;Wd&gt;</a><text>, </text><a hover="Is the 32-bit name of the general-purpose source register, encoded in the &quot;Rn&quot; and &quot;Rm&quot; fields." link="RmRn_option">&lt;Ws&gt;</a><text>, #</text><a hover="For the 32-bit variant: is the amount by which to rotate, in the range 0 to 31, encoded in the &quot;imms&quot; field." link="shift__5">&lt;shift&gt;</a></asmtemplate>
        <equivalent_to>
