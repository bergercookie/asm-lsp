        <box hibit="17" width="4" name="i4" usename="1">
          <c colspan="4"></c>
        </box>
        <box hibit="13" width="2" name="size" usename="1">
          <c colspan="2"></c>
        </box>
        <box hibit="11" width="2" name="opc2" settings="2">
          <c>0</c>
          <c>0</c>
