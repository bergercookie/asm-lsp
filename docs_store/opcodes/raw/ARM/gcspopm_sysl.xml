<instructionsection id="GCSPOPM_SYSL" title="GCSPOPM -- A64" type="alias">
  <docvars>
    <docvar key="alias_mnemonic" value="GCSPOPM"/>
    <docvar key="instr-class" value="system"/>
    <docvar key="isa" value="A64"/>
